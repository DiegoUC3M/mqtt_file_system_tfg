find . -type f -name '.goutputstream-*' -exec rm {} +
find . -type f -name '*.swp' -exec rm {} +

#para chmod y chown:
sudo useradd prueba
sudo passwd prueba
cat /etc/passwd

sudo groupadd grupo_prueba
sudo usermod -a -G grupo_prueba prueba

#para comprobar que funciona chown:
chown prueba:grupo_prueba archivo.txt
chmod 000



